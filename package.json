{
  "private": true,
  "name": "gatsby-blog",
  "description": "Jamstack blog by gatsby",
  "license": "MIT",
  "author": "masaki-koide <ikasama0324@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:masaki-koide/gatsby-blog.git"
  },
  "bugs": {
    "url": "https://github.com/masaki-koide/gatsby-blog/issues"
  },
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop --open",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "NODE_ENV=test stylelint './src/**/*.{ts,tsx}' --cache",
    "lint:js": "eslint './src/**/*.{ts,tsx}' --cache",
    "serve": "gatsby serve",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-plugin-linaria": "2.0.0",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "2.3.5",
    "gatsby-source-contentful": "2.2.10",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "5.25.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "linaria": "1.3.3",
    "path-to-regexp": "^6.1.0",
    "polished": "3.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "babel-jest": "25.4.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-thai-soup": "1.0.4",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "stylelint": "13.3.3",
    "stylelint-config-recess-order": "2.0.4",
    "stylelint-config-recommended": "3.0.0",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "cross-env NODE_ENV=test stylelint --cache"
    ],
    "package.json": "prettier-package-json --write"
  }
}
